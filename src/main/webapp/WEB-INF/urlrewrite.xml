<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
	<rule>
		<from>^/admin/?$</from>
		<to>index.nut</to> 
	</rule>
	<rule>
		<from>^/.*/?$</from>
		<to>/index.nut</to> 
	</rule>
	<rule>
		<from>^/(?!admin/)(.*).html$</from>
		<to>/$1.nut</to> 
	</rule>
</urlrewrite>
